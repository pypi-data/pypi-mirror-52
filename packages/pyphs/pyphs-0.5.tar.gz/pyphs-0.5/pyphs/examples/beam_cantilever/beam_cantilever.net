# cantilever beam
beams.cantilever beam ('nIn', 'nOut'): F=440.0; R=2e-3; E=180e+9; D=1; A=1e0; M=7750.; N=5; z1=0.2; w1=0.015; z2=1.; w2=0.;

# We impose the force on node "nIn" and observe velocity on "nOut"
mechanics.source IN ('nIn', '#'): type=force;
mechanics.source OUT ('nOut', '#'): type=force;
