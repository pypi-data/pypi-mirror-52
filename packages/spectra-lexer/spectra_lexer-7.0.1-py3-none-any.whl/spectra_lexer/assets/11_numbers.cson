# Number rules - since each of these is either special or prefixed with the
# number sign, even a large number of them should not hurt lexer performance.

{
  # special rules
  "1":  ["S",     "1",   ["SPEC"], "1"],
  "2":  ["T",     "2",   ["SPEC"], "2"],
  "3":  ["P",     "3",   ["SPEC"], "3"],
  "4":  ["H",     "4",   ["SPEC"], "4"],
  "5":  ["A",     "5",   ["SPEC"], "5"],
  "0":  ["O",     "0",   ["SPEC"], "0"],
  "6":  ["-F",    "6",   ["SPEC"], "6"],
  "7":  ["-P",    "7",   ["SPEC"], "7"],
  "8":  ["-L",    "8",   ["SPEC"], "8"],
  "9":  ["-T",    "9",   ["SPEC"], "9"],
  
  # single digits
  "1.": ["#S",    "(1)(#:NM)", [], "number 1"],
  "2.": ["#T",    "(2)(#:NM)", [], "number 2"],
  "3.": ["#P",    "(3)(#:NM)", [], "number 3"],
  "4.": ["#H",    "(4)(#:NM)", [], "number 4"],
