  <init-function>glade_python_init</init-function>
  
  <glade-widget-classes>
    <glade-widget-class title="XPL Application Window" name="XPLAppWindow" parent="GtkApplicationWindow" generic-name="xplappwindow"/>
    <glade-widget-class title="XPL Edit Spectrum Dialog" name="XPLEditSpectrumDialog" parent="GtkDialog" generic-name="xpleditspectrumdialog"/>
    <glade-widget-class title="XPL Spectrum TreeStore" name="XPLSpectrumTreeStore" parent="GtkTreeStore" generic-name="xplspectrumtreestore"/>
    <glade-widget-class title="XPL Peak TreeStore" name="XPLPeakTreeStore" parent="GtkTreeStore" generic-name="xplpeaktreestore"/>
    <glade-widget-class title="XPL Canvas" name="XPLCanvas" parent="GtkDrawingArea" generic-name="xplcanvas"/>
    <glade-widget-class title="MPL Canvas" name="MPLCanvas" parent="GtkDrawingArea" generic-name="mplcanvas"/>
    <glade-widget-class title="XPL Plot Toolbar" name="XPLPlotToolbar" parent="GtkToolbar" generic-name="xplplottoolbar"/>
  </glade-widget-classes>
</glade-catalog>
