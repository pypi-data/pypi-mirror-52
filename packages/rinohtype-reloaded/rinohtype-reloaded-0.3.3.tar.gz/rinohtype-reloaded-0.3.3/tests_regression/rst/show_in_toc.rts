[STYLESHEET]
base = sphinx_base14

[VARIABLES]
mono_typeface=Courier
serif_typeface=Times
sans_typeface=Helvetica
thin_black_stroke=0.5pt,#000
blue=#000


[chapter]
page_break = none

[subsection-1a : Section(id='subsection-1a')]
show_in_toc = false

[section-2 : +Section(id='section-2')]
show_in_toc = false
