&FORCE_EVAL
  METHOD FIST
  &MM
    &FORCEFIELD
      PARM_FILE_NAME ../../par_all34_ethers.prm
      PARMTYPE CHM
    &END FORCEFIELD
    &POISSON
      POISSON_SOLVER ANALYTIC
      PERIODIC NONE
      &EWALD
        EWALD_TYPE NONE
      &END EWALD
    &END POISSON
  &END MM
  &SUBSYS
    &CELL
      ABC [angstrom] 30 30 30
      PERIODIC NONE
    &END CELL
    &TOPOLOGY
      COORD_FILE_FORMAT XYZ
      COORD_FILE_NAME ../opt.xyz
      CONN_FILE_FORMAT UPSF
      CONN_FILE_NAME ../init.psf
      EXCLUDE_VDW 1-3
      &CENTER_COORDINATES T
      &END
    &END TOPOLOGY
    &KIND CC33A
      ELEMENT C
    &END KIND
    &KIND CC32A
      ELEMENT C
    &END KIND
    &KIND HCA3
      ELEMENT H
    &END KIND
    &KIND HCA2
      ELEMENT H
    &END KIND
  &END SUBSYS
&END FORCE_EVAL
&GLOBAL
  PROJECT sp
  RUN_TYPE ENERGY_FORCE
  PRINT_LEVEL HIGH
&END GLOBAL

