import sys, pyAMI.app, pyAMI_nika2.config

#############################################################################
# ENTRY POINT                                                               #
#############################################################################

def entry_point():
	app = pyAMI.app.App('nika2', 'text')

	return app.run()

#############################################################################

if __name__ == '__main__':
	sys.exit(entry_point())

