#!/usr/local/bin/python3
# EASY-INSTALL-ENTRY-SCRIPT: 'clientcentral==8.0.1','console_scripts','test'
__requires__ = 'clientcentral==8.0.1'
import re
import sys
from pkg_resources import load_entry_point

if __name__ == '__main__':
    sys.argv[0] = re.sub(r'(-script\.pyw?|\.exe)?$', '', sys.argv[0])
    sys.exit(
        load_entry_point('clientcentral==8.0.1', 'console_scripts', 'test')()
    )
